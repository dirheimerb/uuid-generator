{
  "name": "uuid-wand",
  "version": "1.0.3",
  "description": "A UUID generator library for TypeScript",
  "main": "dist/src/index.js",
  "scripts": {
    "dev": "ts-node example.ts",
    "build": "tsc",
    "docs": "typedoc --out docs src/index.ts",
    "doc:markdown:uuid": "typedoc --plugin typedoc-plugin-markdown --out doc/uuid src/lib/UuidGenerator.ts",
    "doc:markdown:pad": "typedoc --plugin typedoc-plugin-markdown --out doc/pad src/utils/pad.ts",
    "doc:markdown": "typedoc --plugin typedoc-plugin-markdown --out doc src/index.ts",
    "format": "prettier . --write",
    "test": "jest"
  },
  "keywords": [
    "uuid",
    "uuid-generator",
    "uuids",
    "generator",
    "typescript"
  ],
  "author": "Bradley Dirheimer",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/dirheimerb/uuid-generator.git"
  },
  "devDependencies": {
    "@types/jest": "^29.5.6",
    "@types/node": "^20.8.9",
    "jest": "^29.7.0",
    "prettier": "^3.0.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typedoc": "^0.25.2",
    "typedoc-plugin-markdown": "^3.16.0",
    "typescript": "^5.2.2"
  },
  "prettier": {
    "printWidth": 80,
    "singleQuote": true,
    "trailingComma": "all",
    "arrowParens": "always",
    "endOfLine": "lf"
  }
}
